// 既定値を上書きするには、このファイル内にキー バインドを挿入します
[
  {
    "key": "cmd+n",
    "command": "explorer.newFile",
    "when": "explorerViewletFocus"
  },
  {
    "key": "ctrl+n",
    "command": "explorer.newFolder",
    "when": "explorerViewletFocus"
  },
  {
    "key": "ctrl+shift+n",
    "command": "workbench.action.duplicateWorkspaceInNewWindow"
  }
]
